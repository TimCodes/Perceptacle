{
  "name": "synapse-monorepo",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "description": "Synapse diagram builder - monorepo with separated client and server",
  "scripts": {
    "dev": "npm run db:start:dev && concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd packages/server && npm run dev",
    "dev:client": "cd packages/client && npm run dev",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd packages/client && npm run build",
    "build:server": "cd packages/server && npm run build",
    "start:server": "cd packages/server && npm run start",
    "test": "concurrently \"npm run test:server\" \"npm run test:client\"",
    "test:server": "cd packages/server && npm run test",
    "test:client": "cd packages/client && npm run test",
    "db:push": "cd packages/server && npm run db:push",
    "db:studio": "cd packages/server && npm run db:studio",
    "docker:dev": "docker-compose -f docker-compose.dev.yml --env-file .env.development up --build",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:prod": "docker-compose --env-file .env.production up --build -d",
    "docker:prod:down": "docker-compose down",
    "docker:clean": "docker-compose down -v --rmi all",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:server": "docker-compose logs -f server",
    "docker:logs:client": "docker-compose logs -f client",
    "docker:logs:db": "docker-compose logs -f postgres",
    "db:start": "docker-compose --env-file .env.production up postgres -d",
    "db:start:dev": "docker-compose -f docker-compose.dev.yml --env-file .env.development up postgres -d",
    "db:stop": "docker-compose stop postgres",
    "db:stop:dev": "docker-compose -f docker-compose.dev.yml stop postgres",
    "dev:agents": "cd packages/agents && uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "docker:logs:agents": "docker-compose logs -f agents"
  },
